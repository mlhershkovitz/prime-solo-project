SELECT "dog"."name" FROM "attributes" 
JOIN "dog" ON "dog"."id" = "attributes"."dog_id"
WHERE "female" = TRUE || 
WHERE "male" = TRUE || 
WHERE "xsmall" = TRUE || 
WHERE "small" = TRUE || 
WHERE "medium" = TRUE || 
WHERE "large" = TRUE || 
WHERE "xlarge" = TRUE || 
WHERE "shedding" = TRUE || 
WHERE "not shedding" = TRUE || 
WHERE "activity-xlow" = TRUE || 
WHERE "activity-low" = TRUE || 
WHERE "activity-medium" = TRUE || 
WHERE "activity-high" = TRUE || 
WHERE "activity-xhigh" = TRUE || 
WHERE "train-basics" = TRUE || 
WHERE "train-easy" = TRUE || 
WHERE "train-hard" = TRUE || 
WHERE "dogs" = TRUE || 
WHERE "cats" = TRUE || 
WHERE "kids" = TRUE || 
WHERE "other-animals" = TRUE || 
WHERE "single-animal" = TRUE || 
WHERE "dog-park" = TRUE || 
WHERE "crowds" = TRUE || 
WHERE "apartment" = TRUE || 
WHERE "physical-fence" = TRUE || 
WHERE "leash-walking" = TRUE || 
WHERE "dog_id" = TRUE

